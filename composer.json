{
    "name": "daniel-km/omeka-s-module-comment",
    "description": "Module for Omeka S that allows visitors to comment on resources.",
    "type": "library",
    "license": "CECILL-2.1",
    "authors": [
        {
            "name": "Daniel Berthereau",
            "email": "Daniel.packagist@Berthereau.net"
        }
    ],
    "homepage": "https://gitlab.com/Daniel-KM/Omeka-S-module-Comment",
    "support": {
        "issues": "https://gitlab.com/Daniel-KM/Omeka-S-module-Comment/-/issues",
        "forum": "https://forum.omeka.org/"
    },
    "keywords": [
        "Omeka S",
        "module",
        "social",
        "comment",
        "public interaction",
        "crowdsourcing"
    ],
    "autoload": {
        "psr-4": {
            "Comment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CommentTest\\": "tests/CommentTest/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "installer-name" : "Comment"
    }
}
